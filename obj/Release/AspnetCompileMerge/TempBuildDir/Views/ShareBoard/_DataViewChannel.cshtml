 <tr><td colspan="4" style="text-align:center;background-color:rgb(255,255,0);">SUMMARY BY CHANNEL</td></tr>
 @{decimal? t = 0;
    foreach (StrawmanApp.Models.MarketViewChannelModels item in ViewBag.MarketViewChannelsData)
    {
        string _style = "";
        _style = "background-color: rgb(255,0,0);color:rgb(255,255,255);";
        if (item.vchannel == 9) { _style = "background-color: rgb(0,102,255);color:rgb(255,255,255);"; }
        if (item.vchannel == 999999) { _style = "background-color: rgb(0,255,0);color:rgb(0,0,0);"; }  
        <tr class="grid-row" style="text-align:center;@_style"> 
            <td colspan="4" class="grid-cell" style="text-align: right; font-weight:bold;">@Html.DisplayFor(m => item.name)</td>
        </tr>         
        t = item.vgroup;
    }
    }