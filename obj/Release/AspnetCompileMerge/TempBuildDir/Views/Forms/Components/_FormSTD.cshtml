@model StrawmanApp.Models.FormModel
<div class="form-horizontal">
@using (Ajax.BeginForm(Model.action,Model.controller,new AjaxOptions{ OnSuccess = "FormSucces", OnFailure="FormFailure"}, new { data_form_id = Model.from_id }))
{
    <div class="col-lg-6">
    @foreach (StrawmanApp.Models.FormElement element in Model.objects.Where(m=>m.column==1).Select(m=>m).ToList())
    {
        <div class="form-group col-lg-12">@{Html.RenderPartial(element.partial_view, element.model);}</div>
    }
    </div>
    <div class="col-lg-6">
    @foreach (StrawmanApp.Models.FormElement element in Model.objects.Where(m=>m.column==2).Select(m=>m).ToList())
    {
        <div class="form-group col-lg-12">@{Html.RenderPartial(element.partial_view, element.model);}</div>
    }
    </div>
}
</div>