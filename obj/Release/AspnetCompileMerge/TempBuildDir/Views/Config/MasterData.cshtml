@{    
    ViewBag.Title = "MasterData";
    Layout = "~/Views/Shared/_StrwLayout.cshtml";
}
<div class="config" style="height:100%;">
    <div style="width: 100%; height: 42px; box-shadow: 0px 1px 3px rgb(180,180,180); background-color: rgb(246, 246, 246);">
        @{Html.RenderAction("GetButtonsGroup", "FormUtil", new { channel = ViewBag.Channel });}
    </div>
    <div class="wrapper config" style="top: 45px; width: 100%; bottom: 0px; overflow: scroll; position: absolute; margin:0;padding: 0 10px;">
        @{Html.RenderAction("MasterDataGrid", new{_channel = ViewBag.Channel, grid_page=1});}
    </div>
    @{Html.RenderPartial("~/Views/Shared/_Modal.cshtml", new ViewDataDictionary {{"modal_name","modal_new_item"}});}
</div>
