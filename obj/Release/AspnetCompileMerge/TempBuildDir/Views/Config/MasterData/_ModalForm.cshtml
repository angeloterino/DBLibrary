@model  StrawmanApp.Models.FormModel
<div class="modal-content wrapper">
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">×</span>
        </button>
        <h4 class="modal-title" id="@ViewBag.ModalId">@ViewBag.ModalTitle</h4>
      </div>
    <div class="modal-body">
    @if (ViewBag.NavigationButtons != null)
    {
        Html.RenderAction("GetButtonsGroupMasterData", "FormUtil", new { name = ViewBag.NavigationButtons });
    }
        <div class="form_wrapper">
        @{
            StrawmanApp.Models.FormModel form = (StrawmanApp.Models.FormModel)Model;
            Html.RenderPartial("~/Views/Forms/Components/_FormSTD.cshtml", form);
        }

        </div>
    </div>
    
</div>

