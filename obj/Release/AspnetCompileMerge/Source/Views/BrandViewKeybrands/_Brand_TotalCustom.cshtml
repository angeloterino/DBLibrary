@{  
    Html.RenderPartial("~/Views/StrawmanViews/MasterTable/_BlankRow.cshtml", new ViewDataDictionary{{"colspan",3}});
    Html.RenderPartial("~/Views/StrawmanViews/MasterTable/_TitledRow.cshtml", new ViewDataDictionary { { "colspan", 3 } });

    string _styleView = "_TrStyleKeybrands";
    string _style = "text-align:center;";
    foreach (StrawmanApp.Models.StrawmanViewSTDModel item in ViewBag.BrandViewKeybrandsTotalCustom)
    {
        ViewDataDictionary _styleData = new ViewDataDictionary { { "_style", _style }, { "id", item.vid } };
        StrawmanApp.Models.StrawmanSTDView view = new StrawmanApp.Models.StrawmanSTDView
        {
            market = item.market,
            brand = item.brand,
            channel = item.channel,
            vgroup = item.vgroup,
            vorder = item.vorder,
            col1 = (decimal?)item.col1,
            col2 = (decimal?)item.col2,
            col3 = (decimal?)item.col3
        };
        string[] _colData = new string[] { Html.DisplayFor(m => item.col1).ToString(), Html.DisplayFor(m => item.col2).ToString(), Html.DisplayFor(m => item.col3).ToString() };
        string[] _colType = new string[] { "amount", "amount", "amount" };
        Html.RenderPartial("~/Views/StrawmanViews/_StrawmanViewSTD.cshtml", new StrawmanApp.Models.StrawmanSTDViewModel { item = view, itemaux = view }, new ViewDataDictionary { { "_colData", _colData }, { "_colType", _colType }, { "styleView", _styleView }, { "styleData", _styleData } });
    }
}