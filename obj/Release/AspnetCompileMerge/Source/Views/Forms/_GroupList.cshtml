@model StrawmanApp.Entities.GroupListModel

@using (Ajax.BeginForm("InsertField", "Forms",new AjaxOptions {HttpMethod = "Post"})){
    List<StrawmanApp.Entities.GodzillaEntity.GROUP_MASTER> lst = Model.datamaster.ToList();
    if (lst != null && lst.Count > 0)
    {

    foreach (StrawmanApp.Entities.GodzillaEntity.GROUP_MASTER item in lst)
    {
        string link_text = (item.NAME == null)? "NULL":item.NAME;
        
        <a class="list-group-item" btn-type = "edit-group" btn-target-id = "self"  btn-target-url = "SelectGroup" btn-param =  "@item.ID">@link_text</a>

    }

    }
    else
    {
        <div class="list-group-item disabled">No existen registros en la tabla.</div>
    }   
}