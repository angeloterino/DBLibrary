@{
    ViewBag.Title = "LoaderPreview";
    Layout = "~/Views/Shared/_StrwLayout.cshtml";
}
@model List<StrawmanApp.Entities.ExcelLoader>

@if(ViewData["Model"] == null){ViewData.Add("Model", Model);}
@using GridMvc.Html

@using (Ajax.BeginForm("SaveData", "Forms", new AjaxOptions { OnSuccess = "UploadSuccess" }))
{
@Html.Grid((List<StrawmanApp.Entities.ExcelLoader>)ViewData["Model"]).Columns(columns =>
{
    columns.Add(m => m.col1).RenderValueAs(o=> Html.TextBoxFor(m=>o.col1).ToHtmlString());
    columns.Add(m => m.col2).RenderValueAs(o => Html.TextBoxFor(m => o.col2).ToHtmlString());
    columns.Add(m => m.col3).RenderValueAs(o => Html.TextBoxFor(m => o.col3).ToHtmlString());
    columns.Add(m => m.col4).RenderValueAs(o => Html.TextBoxFor(m => o.col4).ToHtmlString());
    columns.Add(m => m.col5).RenderValueAs(o => Html.TextBoxFor(m => o.col5).ToHtmlString());
    columns.Add(m => m.col6).RenderValueAs(o => Html.TextBoxFor(m => o.col6).ToHtmlString());
    columns.Add(m => m.col7).RenderValueAs(o => Html.TextBoxFor(m => o.col7).ToHtmlString());
    columns.Add(m => m.col8).RenderValueAs(o => Html.TextBoxFor(m => o.col8).ToHtmlString());
    columns.Add(m => m.col9).RenderValueAs(o => Html.TextBoxFor(m => o.col9).ToHtmlString());
    columns.Add(m => m.col10).RenderValueAs(o => Html.TextBoxFor(m => o.col10).ToHtmlString());
    columns.Add(m => m.col11).RenderValueAs(o => Html.TextBoxFor(m => o.col11).ToHtmlString());
    columns.Add(m => m.col12).RenderValueAs(o => Html.TextBoxFor(m => o.col12).ToHtmlString()); 
}).WithPaging(30);
  <input type ="submit" value="Guardar" />
}