@model StrawmanApp.Models.UsersManageModel
@{
    ViewBag.Title = "UserManagment";
    Layout = "~/Views/Shared/_StrwLayout.cshtml";
}

<h2>Manage User Accounts</h2>
<div class="wrapper">
    @Html.LabelFor(m => Model.user)
    @Html.DropDownListFor(m=>Model.user, new SelectList(Model.UsersList,"Value","Text"))
    <div class="tabs wrapper"></div>
    <div class="form wrapper">
        @{Html.RenderAction("UsersManagement", new { _user = Model.UsersList.FirstOrDefault().Text, _channel = 1 });}
    </div>
</div>
