@model List<System.Web.Mvc.SelectListItem>
<h3 class="col-lg-2" id="labelSelect" style="font-size: 16px; margin-top: 1.5%;">@ViewBag.SelectListLabel</h3>
<div class="col-lg-10">@Html.DropDownList((string)ViewBag.SelectListName, (List<System.Web.Mvc.SelectListItem>)Model, new { @class = "form-control",@data_type="edit-select"})</div>
<div class="clearfix" style="clear:both"></div>