@{
    var _style = ViewData["_style"] == null ? "" : ViewData["_style"];
    var _style_t = "";
    var _id = int.Parse(ViewData["id"].ToString());
    if (_id == 999) { _style_t = "background-color: rgb(0,102,255);color:rgb(255,255,255);"; }
    if (_id == 999999) { _style_t = "background-color: rgb(0,255,0);color:rgb(0,0,0);"; }
    _style += _style_t;
}
style = "@_style"