@{
    ViewBag.Title = "BoyOTC";
    Layout = "~/Views/Shared/_StrwLayout.cshtml";
}
<div class="load-mask" style="position:absolute; top:0;left:0;width:100%;height:100%;background-color:#CCC;z-index:1000;">
<div style="position:relative; top:50%; margin:-160px auto 0 auto; background-color:#FFF; background-image: url('@Url.Content("~/Images/Godzilla.jpg")'); background-size:cover; width:620px;height:320px;box-shadow: 8px 8px 12px rgb(120,120,130);">
<div class="text loader" style="float:right;margin-right:15%; margin-top:20%;position:relative;">
    <span style="font-size:2.5em;">Building...</span>
    <div class="loading-bar" style="border:2px solid rgb(0,0,220); width:0px; height:1px;"></div>
    <div class="loading-info" style="width:100%; text-align: center; font-size:1em; color:rgb(0,0,0); position:absolute; left:0;"></div>
</div>
</div>
</div>
<div style="position:relative;font-size: .8em;" id="content" class="mastertable">
