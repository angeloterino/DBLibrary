@{    
    Layout = "~/Views/Shared/_StrwLayout.cshtml";
}
@{Html.RenderPartial("~/Views/Shared/_LoadMask.cshtml");}
<div class="letters wrapper mastertable">
<div id="ManagementLetter_Data">
</div>
<div class="info" style="margin:10px;">
    <table style="text-align:center;font-size:1.3em;">
        <tr>
            <td style="width:60px;font-weight:bold;">Source:</td>
            <td></td>
        </tr>
        <tr>
            <td style="font-weight:bold;text-align:right;"><span>Nielsen:</span></td>
            <td style="width:120px;"><span class="period month">@ViewBag.PeriodMonth</span></td>
        </tr>
        <tr>
            <td style="font-weight:bold;text-align:right;"><span>IMS:</span></td>
            <td><span class="period month">@ViewBag.PeriodMonth</span></td>
        </tr>
        <tr>
            <td style="font-weight:bold;text-align:right;"><span>NTS:</span></td>
            <td><span class="period month">@ViewBag.PeriodMonth</span></td>
        </tr>
    </table>
</div>
<div id="ManagementLetter_Comments">
</div>
</div>
