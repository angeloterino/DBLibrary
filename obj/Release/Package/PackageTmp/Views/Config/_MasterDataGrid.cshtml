@model List<StrawmanApp.Models.MasterDataModels>
@using GridMvc.Html;    
@Html.Grid((List<StrawmanApp.Models.MasterDataModels>)Model).Columns(columns =>
           {
               columns.Add(foo => foo.market_name).Titled("MARKET").Sortable(true);
                columns.Add(foo => foo.brand_name).Titled("BRAND").Sortable(true);
                columns.Add(foo => foo.group_name).Titled("GROUP").Sortable(true);
                columns.Add(foo => foo.channel_name).Titled("CHANNEL").Sortable(true);
                columns.Add(foo => foo.source).Titled("SOURCE").Sortable(true);
                columns.Add().Encoded(false).Sanitized(false).SetWidth(30).RenderValueAs(o => @<button class="btn btn-success" type="button" data-type="edit" data-target-market="@o.id" data-target-brand="@o.brand" data-target-channel="@o.channel" class="btn btn-default">Edit</button>);
    }).Selectable(false).WithPaging(20)