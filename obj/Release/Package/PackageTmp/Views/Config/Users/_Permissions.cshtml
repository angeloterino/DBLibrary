@model StrawmanApp.Models.UsersManageModel
<h3>Menu Access</h3>
@{
    using (Ajax.BeginForm("UpdateMenuAccess", new AjaxOptions { HttpMethod = "Post", OnSuccess = "UpdateSuccess", OnFailure = "UpdateFailure" }))
    {
        Html.RenderPartial("Users/_MenuAccess", Model.MenuAccess);
        <input type="submit" class="btn btn-default" value="Save" id="SaveUserMenuAccess" style="border: 1px solid rgb(204, 204, 204); font-size: 14px; font-weight: normal;"/>
        @Html.HiddenFor(m => Model.user);
    }
}
<h3>Editable Views Access</h3>
@Html.LabelFor(m => Model.ViewTable)
@Html.DropDownListFor(m=>Model.ViewTable, new SelectList(Model.ViewsList,"Value","Text"))
@{Html.RenderPartial("Users/_ViewAccessForm", Model);}