@{
     Html.RenderPartial("~/Views/StrawmanViews/MasterTable/_BlankRow.cshtml", new ViewDataDictionary{{"colspan",4}});
     Html.RenderPartial("~/Views/StrawmanViews/MasterTable/_TitledRow.cshtml", new ViewDataDictionary { { "colspan", 4 }, { "bg_color", "rgb(255,255,0)" }, { "text", "SUMMARY BY FRANCHISE" } });     
     decimal? t = 0;
   foreach (StrawmanApp.Models.MarketViewChannelModels item in ViewBag.ViewFranchiseData)
    {
        string _style = "";
        if (item.vorder == item.vgroup) { _style = "background-color: rgb(128,0,128);color:rgb(255,255,255);"; }
        if (item.vhas_child == "Y") { _style = "background-color: rgb(255,192,0);font-size:bold;"; }
        if (item.vid == item.vparent) { _style = "background-color: rgb(255,0,0);color:rgb(255,255,255);"; }
        if (item.vorder == 999999) { _style = "background-color: rgb(0,255,0);color:rgb(0,0,0);"; }            
            <tr class="grid-row" style="text-align:center;@_style"> 
                <td class="grid-cell" style="text-align: right; font-weight:bold;">@Html.DisplayFor(m => item.name)</td>
            </tr>        
        t = item.vgroup;        
    }
}