@{
    string _style = "", _class = "class=\"", _attr = "", _data = "", _data_val = "data-value=\"";
    if (ViewData["style"] != null) { _style = "style = \"" + ViewData["style"].ToString() +"\""; }
    if (ViewData["class"] != null) { 
        _class += ViewData["class"].ToString() + "\""; 
    }else{
        _class += "grid-cell\"";
    }
    if (ViewData["attr"] != null) { _attr = "" + ViewData["attr"].ToString(); }
    if (ViewData["data"] != null) { _data = "" + ViewData["data"].ToString(); }
    string _td = "<td " + _class + " " + _style + " " + _attr + ">" + _data + "</td>";
}
@Html.Raw(_td)
