@model StrawmanApp.Models.ChartGenericModel 
<td style="text-align:right; width:auto;width:5%;">@Html.DisplayFor(m =>Model.channel_name)</td>
<td style="width:1%; border: 1px solid rgb(0,0,0); border-left:0;"></td>
@{
    StrawmanApp.Models.ChartGenericModel item = (StrawmanApp.Models.ChartGenericModel)Model;
    if(item._size != null){
            Html.RenderPartial("~/Views/ChartGeneric/_Size.cshtml",item);
    }else{
            Html.RenderPartial("~/Views/ChartGeneric/_Share.cshtml", item);
    }
} 
