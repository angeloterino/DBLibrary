<tr><td colspan="5" style="border:0; font-size:0.5em;">&nbsp;</td></tr>
@{  decimal? t = 0;    
    foreach (StrawmanApp.Models.Market_PCVSPYModels item in ViewBag.MarketPCVSPY)
    {
        string _style = "";
        if(item.vorder == item.vgroup) {_style = "background-color: rgb(128,0,128);color:rgb(255,255,255);";}
        if (item.vorder > item.vgroup) { _style = "background-color: rgb(255,0,0);color:rgb(255,255,255);"; }       
            <tr class="grid-row" style="text-align:center;@_style">     
                <td class="grid-cell">@Html.DisplayFor(m=>item.pcvspycol1)%</td>
                <td class="grid-cell">@Html.DisplayFor(m=>item.pcvspycol2)%</td>
                <td class="grid-cell">@Html.DisplayFor(m=>item.pcvspycol3)%</td>    
                <td class="grid-cell">@Html.DisplayFor(m=>item.pcvspycol4)%</td>    
                <td class="grid-cell">@Html.DisplayFor(m=>item.pcvspycol5)%</td>    
            </tr>
        if (item.vorder > item.vgroup) {
            <tr class="grid-row" style="text-align:center;"> 
                <td colspan="3" class="grid-cell" style="text-align: right; font-weight:bold; border:0;">&nbsp;</td>
            </tr>  
        }            
        t = item.vgroup;
    } 
}