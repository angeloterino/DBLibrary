@model dynamic
<tr class="grid-row" style="text-align:center;"> <td colspan="3" class="grid-cell" style="text-align: right; font-weight:bold; border:0;">&nbsp;</td></tr>
<tr><td colspan="6">&nbsp;</td></tr>
@{  decimal? t = 0;
    List<StrawmanApp.Entities.ShareBoardModel> lst = (List<StrawmanApp.Entities.ShareBoardModel>)Model;
    foreach (StrawmanApp.Models.MarketViewChannelModels item in ViewBag.MarketViewFranchiseData)
    {
        StrawmanApp.Entities.ShareBoardModel data = lst.Find(m => m.brand == item.vid && m.order == item.vorder);
        string _style = "";
        if (item.vorder == item.vgroup) { _style = "background-color: rgb(128,0,128);color:rgb(255,255,255);"; }
        if (item.vhas_child == "Y") { _style = "background-color: rgb(255,192,0);font-size:bold;"; }
        if (item.vid == item.vparent) { _style = "background-color: rgb(255,0,0);color:rgb(255,255,255);"; }
        if (item.vorder == 999999) { _style = "background-color: rgb(0,255,0);color:rgb(0,0,0);"; }
        if (data == null) { data = new StrawmanApp.Entities.ShareBoardModel(); }         
            <tr class="grid-row" style="text-align:center;@_style">     
                <td class="grid-cell" style ="@_style">@Html.DisplayFor(m => data.mat_col1)</td>
                <td class="grid-cell" style ="@_style">@Html.DisplayFor(m => data.mat_col2)</td>
                <td class="grid-cell" style ="@_style">@Html.DisplayFor(m => data.mat_col3)</td>
                <td class="grid-cell" style ="@_style">@Html.DisplayFor(m => data.mat_col4)</td>
                <td class="grid-cell" style ="@_style">@Html.DisplayFor(m => data.mat_col5)</td>
                <td class="grid-cell" style ="@_style">@Html.DisplayFor(m => data.mat_col6)</td>   
            </tr>
        t = item.vgroup;
    } 
}