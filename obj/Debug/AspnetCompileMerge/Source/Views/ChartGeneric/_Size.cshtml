@model StrawmanApp.Models.ChartGenericModel
@{
    StrawmanApp.Models.ChartGenericModel item = Model;
    <td data-type="size">
        <div style="background-color:rgb(0,204,255);width:@Html.DisplayFor(m => item._size_pc).ToString().Replace(',', '.');float:left;height:60px;border:1px solid rgb(0,0,0);border-left:0;"></div><span style="margin-top: 6%; margin-left: 2%; float: left;">@Html.DisplayFor(m => item._size)</span>
    </td>    
}