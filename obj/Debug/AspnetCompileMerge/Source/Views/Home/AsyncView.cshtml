@{
    ViewBag.Title = "AsyncView";
    Layout = "~/Views/Shared/_StrwLayout.cshtml";
}
<script language= "javascript">
    $(document).ready(function () {
        $('.grid-wrap').css({ 'height': '100%' });
        $('.loader').clone().css({ 'display': 'block' }).appendTo('.grid-wrap');
