@model List<StrawmanApp.Models.MasterDataModels>
@using GridMvc.Html;    
@Html.Grid((List<StrawmanApp.Models.MasterDataModels>)Model).Columns(columns =>
           {
                columns.Add(foo => foo.brand_name).Titled("NAME").Sortable(true);
                columns.Add(foo => foo.config).Titled("CONFIG").Sortable(true);
                columns.Add(foo => foo.source).Titled("SOURCE").Sortable(true);
                columns.Add().Encoded(false).Sanitized(false).SetWidth(30).RenderValueAs(o => @<button type="button" data-type="edit" data-target-id="@o.id" class="btn btn-default">Edit</button>);
    }).WithPaging(20)
