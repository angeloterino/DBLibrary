@model StrawmanApp.Models.ChartGenericModel
@{
    StrawmanApp.Models.ChartGenericModel item = Model;
<td  data-type="share"> 
    @if(item!= null){
    <div class="pc"><div style="background-color:rgb(255,0,255);width:@Html.DisplayFor(m => item._share_p_pc).ToString().Replace(',', '.');float:left; height:40px;"></div><span style="margin-top: 6%; margin-left: 2%; float: left;">@Html.DisplayFor(m => item._share_p)</span></div>
    <div class="pc" style="clear:both;"><div style="background-color:rgb(0,0,255);width:@Html.DisplayFor(m => item._share_l_pc).ToString().Replace(',', '.');float:left; height:40px;"></div><span style="margin-top: 6%; margin-left: 2%; float: left;">@Html.DisplayFor(m => item._share_l)</span></div>
    }else{
        <span>&nbsp;</span>
    }
</td>
}