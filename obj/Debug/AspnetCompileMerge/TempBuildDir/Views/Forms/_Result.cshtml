@model List<StrawmanApp.Entities.BoyConfigModel>
@ViewBag.Result
@using StrawmanApp.Helpers
<div>
<ul>
@foreach (StrawmanApp.Entities.BoyConfigModel b in Model){
<li>@Ajax.ActionLink(b.market_name.ToString() + " / " + b.brand_name.ToString(),
                       "GetDataEdit",
                       new {id=b.ID, market = b.market},
                       new AjaxOptions{
                           InsertionMode=InsertionMode.Replace,
                           UpdateTargetId="editForm"
                       })</li>
}
@Ajax.ActionLink(MessageByLanguage.Add,"InsertData","Forms",new {name = Model.First().name, order = (int)Model.First().vorder},new AjaxOptions{InsertionMode = InsertionMode.Replace,UpdateTargetId="editForm"})
</ul>
<div id="editForm"></div>
</div>