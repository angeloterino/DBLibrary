@model List<StrawmanApp.Entities.BoyConfigModel>
@{
    ViewBag.Title = "Forms";
    Layout = "~/Views/Shared/_StrwLayout.cshtml";
}
@foreach (StrawmanApp.Entities.BoyConfigModel b in Model)
{ 
    <span>
    @Html.DisplayFor(m => b.channel)
    </span>
    <span>@Html.DisplayFor(m => b.market)</span>
    <span>        
    @Ajax.ActionLink(b.name.ToString()
                     ,"EditForm"
                     ,new {name = b.name, order = (int)b.vorder}
                     ,new AjaxOptions { InsertionMode=InsertionMode.Replace, UpdateTargetId = "result"})
    </span>
        
    <br />
}
    <div id="result"></div>
