@model List<StrawmanApp.Entities.GodzillaEntity.GROUP_MASTER>
<div class="panel-heading"><h3 class="panel-title">Artículos</h3></div>
@using (Ajax.BeginForm("InsertItem", "Forms", new AjaxOptions { HttpMethod = "Post" }))
{
<div class="panel-body" id="list-items">
    @foreach (StrawmanApp.Entities.GodzillaEntity.GROUP_MASTER item in Model)
    {
        string link_text = (item.NAME == null)? "NULL":item.NAME;
        <a class="list-group-item" btn-type = "edit-group-item" btn-target-id = "new-group"  btn-target-url = "SelectItemGroup" btn-param =  "@item.ID">@link_text</a>
    }
</div>
<div class="panel-footer">
<a class="btn btn-default" style="border:1px solid #ccc;" btn-type="new-item" btn-target-id="new-group" btn-target-url="AddItem">Añadir</a>
</div>
}