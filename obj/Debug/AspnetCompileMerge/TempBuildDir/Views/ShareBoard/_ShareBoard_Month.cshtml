@model List<StrawmanApp.Models.MarketDataModels>
@{Html.RenderPartial("~/Views/StrawmanViews/MasterTable/_BlankRow.cshtml", new ViewDataDictionary{{"colspan",6}});}
@{  
    List<StrawmanApp.Entities.ShareBoardModel> items = (List<StrawmanApp.Entities.ShareBoardModel>)ViewBag.MarketMonth;
    foreach (StrawmanApp.Models.MarketDataModels itemaux in Model)
    {
        StrawmanApp.Entities.ShareBoardModel item = items.Find(m => m.brand == itemaux.brand && m.market == itemaux.market && m.channel == itemaux.channel);
        bool _last = item == null ? false : Model.Last(m => m.channel == itemaux.channel).brand == item.brand;
        StrawmanApp.Models.StrawmanSTDView viewaux = new StrawmanApp.Models.StrawmanSTDView
        {
            market = itemaux.market,
            brand = itemaux.brand,
            vgroup = itemaux.vgroup,
            vorder = itemaux.vorder,
            col1 = item == null ? null : (decimal?)item.month_col1,
            col2 = item == null ? null : (decimal?)item.month_col2,
            col3 = item == null ? null : (decimal?)item.month_col3,
            col4 = item == null ? null : (decimal?)item.month_col4,
            col5 = item == null ? null : (decimal?)item.month_col5,
            col6 = item == null ? null : (decimal?)item.month_col6,
        };
        StrawmanApp.Models.StrawmanSTDView view = item==null?null: new StrawmanApp.Models.StrawmanSTDView
        {
            market = item.market,
            brand = item.brand,
            vgroup = itemaux.vgroup,
            vorder = itemaux.vorder,
            col1 = (decimal?)item.month_col1,
            col2 = (decimal?)item.month_col2,
            col3 = (decimal?)item.month_col3,
            col4 = (decimal?)item.month_col4,
            col5 = (decimal?)item.month_col5,
            col6 = (decimal?)item.month_col6
        };
        string[] _colData = item == null ? null : new string[] { 
                                                            Html.DisplayFor(m => item.month_col1).ToString(), 
                                                            Html.DisplayFor(m => item.month_col2).ToString(), 
                                                            Html.DisplayFor(m => item.month_col3).ToString(), 
                                                            Html.DisplayFor(m => item.month_col4).ToString(),
                                                            Html.DisplayFor(m => item.month_col5).ToString(),
                                                            Html.DisplayFor(m => item.month_col6).ToString()
                                                            };
        string[] _colType = new string[] { "pc", "pc", "pc","pc","amount","amount" };
        string[] _colStyle = new string[] { 
                item == null ? null : item.month_col1 < 0 && (itemaux.vorder != itemaux.vgroup && itemaux.vorder <= itemaux.vgroup) ? "color:rgb(255,0,0);" : "",
                item == null ? null : item.month_col2 < 0 && (itemaux.vorder != itemaux.vgroup && itemaux.vorder <= itemaux.vgroup) ? "color:rgb(255,0,0);" : "",
                item == null ? null : item.month_col3 < 0 && (itemaux.vorder != itemaux.vgroup && itemaux.vorder <= itemaux.vgroup) ? "color:rgb(255,0,0);" : "",
                item == null ? null : item.month_col4 < 0 && (itemaux.vorder != itemaux.vgroup && itemaux.vorder <= itemaux.vgroup) ? "color:rgb(255,0,0);" : "",
                item == null ? null : item.month_col5 < 0 && (itemaux.vorder != itemaux.vgroup && itemaux.vorder <= itemaux.vgroup) ? "color:rgb(255,0,0);" : "",
                item == null ? null : item.month_col6 < 0 && (itemaux.vorder != itemaux.vgroup && itemaux.vorder <= itemaux.vgroup) ? "color:rgb(255,0,0);" : "",
                };
        Html.RenderPartial("~/Views/StrawmanViews/_StrawmanViewSTDShareBoard.cshtml", new StrawmanApp.Models.StrawmanSTDViewModel { item = view, itemaux = viewaux }, new ViewDataDictionary { { "_colData", _colData }, { "_colType", _colType }, { "_colStyle", _colStyle }, { "_last", _last } });
    } 
}