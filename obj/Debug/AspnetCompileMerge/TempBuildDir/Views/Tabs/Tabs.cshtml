  @model List<StrawmanApp.Models.TabItem>
    <ul id="menu" style="font-size:1.5em;">
        @{ 
            List<StrawmanApp.Models.TabItem> lst = Model;            
            foreach(StrawmanApp.Models.TabItem item in lst){
            string tabUrl = item.controller + "/" + item.url;
                if (item.tab_url != tabUrl){
                    <li style="padding: 10px; border-width: 0px 1px 0px 0px; border-style:solid; border-color: rgb(0, 0, 0) rgb(204, 200, 204) rgb(0, 0, 0) rgb(0, 0, 0); vertical-align: middle; float: left; display: block; min-height: 40px;">
                    @Html.ActionLink(item.name, item.url, item.controller,null, new { role = "tabitem" })                    
                    </li>
                }
                else
                {
                    <li style="border-width: 1px; border-style: solid; border-color: rgb(255, 255, 255) rgb(0, 0, 0); padding: 10px; margin-top: -1px; vertical-align: middle; float: left; display: block; min-height: 40px; background-color: rgb(255, 255, 255);">
                    @Html.Label(item.name, new { @style = "font-size:1em;" })
                    </li>
                }
            }
                        
        }
    </ul>     