@{
    ViewBag.Title = "BoyMassMarket";
    Layout = "~/Views/Shared/_StrwLayout.cshtml";
    
}
@{Html.RenderPartial("~/Views/Shared/_LoadMask.cshtml");}
@{
    int rownum = 0;
<div style="position:relative;font-size: .8em;display:table;" id="content" class="mastertable">
@foreach(StrawmanApp.Models.BoyMassMarketModels item in ViewBag.BoyMasterData){
    Html.RenderPartial("~/Views/BoyMassMarket/_BoyRow.cshtml", new ViewDataDictionary { { "rownum", rownum },{"editable", item.brand < 9000 && item.market < 9000} });
    rownum++;
}
<div class="clear" style="clear:both;"><input type="hidden" id="channelID" value="@ViewBag.Channel" /></div>
@{Html.RenderPartial("~/Views/Config/BOY/_EditEnabled.cshtml");}
@{Html.RenderPartial("~/Views/Shared/_Modal.cshtml", new ViewDataDictionary {{"modal_name","modal_comments"}});}
</div>
}
