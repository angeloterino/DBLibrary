@model List<StrawmanApp.Models.MarketViewChannelModels>
@{ Html.RenderPartial("~/Views/StrawmanViews/MasterTable/_TitledRow.cshtml", new ViewDataDictionary { { "colspan", 4 },{"bg_color","rgb(255,255,0)"},{"text","SUMMARY BY CHANNEL"} });
    decimal? t = 0;
    foreach (StrawmanApp.Models.MarketViewChannelModels item in Model)
    {
        string _style = "text-align:center;" + item.style;
        <tr class="grid-row" @{Html.RenderPartial("~/Views/StrawmanViews/MasterTable/_TrStyleChannel.cshtml", new ViewDataDictionary { { "_style", _style }, { "channel", item.vchannel }});}> 
            <td colspan="4" class="grid-cell" style="text-align: right; font-weight:bold;">@item.name</td>
        </tr>         
        t = item.vgroup;
    }
}