@model List<StrawmanDBLibray.Entities.v_TMP_MASTER_DATA>
@{
<table class="table table-striped table-bordered table-hover" style="font-size:12px;border:1px solid rgb(150, 150, 150);">
    <thead style="color: white; background-color: rgba(122, 192, 218, 1);">
        <tr style="background-color: rgba(56, 151, 199, 1);"><th rowspan="2">Market Name</th><th rowspan="2">Brand Name</th><th colspan ="2">IMS/Nielsen Data</th><th colspan="2">NTS Data</th><th rowspan="2">Options</th></tr>
        <tr><th>Market</th><th>Brand</th><th>NTS Name</th><th>Total NTS</th></tr>
    </thead>
    <tbody>
    @foreach(StrawmanDBLibray.Entities.v_TMP_MASTER_DATA item in Model){
        Html.RenderAction("GetItemsConfigData", new { _market = item.MARKET, _brand = item.BRAND, _channel = item.CHANNEL });
    }
    </tbody>
</table>
}