@{
  int vb_index = 0;
  
  List<StrawmanApp.Models.BoyMassMarketModels> lstaux = (List<StrawmanApp.Models.BoyMassMarketModels>)ViewBag.BoyYTD;
  int _year = (ViewBag.YearPeriod == null) ? 15 : int.Parse(new DateTime((int)ViewBag.YearPeriod, 1, 1).ToString("yy"));
  foreach (StrawmanApp.Models.BoyMassMarketModels yitemaux in Model)
    {

        StrawmanApp.Models.BoyMassMarketModels item = lstaux.Find(m => m.brand == yitemaux.brand && m.market == yitemaux.market && m.channel == yitemaux.channel);
        Html.RenderPartial("~/Views/BoyMassMarket/_BOYStdView.cshtml", item, new ViewDataDictionary { { "yitemaux", yitemaux }, { "header", new[] { "TO GO " + (_year - 1).ToString(), "TO GO " + (_year).ToString(), "% Growth VYA" } } });
        
        vb_index++;
    }        
}
