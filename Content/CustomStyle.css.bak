body {
}
td, th {
    padding: 0px;
}
table {
    margin-top: 0;    
}
.table > tbody > tr > td, .table > tbody > tr > th, .table > tfoot > tr > td, .table > tfoot > tr > th, .table > thead > tr > td, .table > thead > tr > th {
    padding: 2px;
    line-height: 1.42857;
    vertical-align: top;
    border: 1px solid rgb(0,0,0);
}
.table > thead > tr > th {
    vertical-align: middle;
    border: 1px solid rgb(0,0,0);
    text-align: center;
}
.table-striped > tbody > tr:nth-of-type(2n+1) {
    background-color: transparent;
}
.table {
    width: 100%;
    max-width: 100%;
    margin-bottom: 0;
    border: 0;
    font-size: 11px;
    font-family: Helvetica, Verdana, San-Serif !important;
    background-color:rgb(255,255,255);
}
#body
{
    background-color:rgb(255,255,255);
}
.grid-wrap
{
    border-top: 1px solid rgb(0,0,0);
}
#rotate{
    -moz-border-radius: 25px/25px;
    -webkit-border-radius: 25px 25px;
    border-radius: 40px/40px;
    border:solid 1px rgb(255,255,255); 
    width:50px;
    height:50px;
    -webkit-animation: rotation 2s linear infinite;
    -moz-animation: rotation 2s linear infinite;
    -ms-animation: rotation 2s linear infinite;
}

@-webkit-keyframes rotation {
    0%   { -webkit-transform: rotate(0deg); }
    100% { -webkit-transform: rotate(360deg); }
}
@-moz-keyframes rotation {
    0%   { -moz-transform: rotate(0deg); }
    100% { -moz-transform: rotate(360deg); }
}
@-ms-keyframes rotation {
    0%   { -ms-transform: rotate(0deg); }
    100% { -ms-transform: rotate(360deg); }
}
.boytable
{
    font-size:1.3em;
}
.boytable .grid-cell
{
    text-align:right;padding:3px;
    border:0;
}
.boytable > thead > tr > th
{
    color:rgb(255,255,255);
    border:0;
    border-bottom: 1px solid rgb(0,0,0);
    min-width:95px;
    font-weight:normal;
    font-size:1.1em;
}
.collapser
{
   cursor: pointer;
   position:absolute; top:0; right:0; min-width:20px; padding:0px 4px;font-size:1.5em;font-weight:bold; border-left:1px solid rgb(0,0,0); border-right:1px solid rgb(0,0,0); height:39px;
}
.chart
{
    width:97%;
    font-weight:bold;
    margin-bottom:20px !important;
}
.chart tbody>td>div
{
    width:100%;
}
.chart tbody>tr
{
    height:160px;
}
.chart thead tr th
{
    text-align:center;
}
.chart thead tr>th
{
    width: 11.5%;
}
.chart .pc
{
    float:left;
    margin-left:50%;
    width:50%;
    white-space:nowrap;
}
.chart .pc>div
{
    border:1px solid rgb(0,0,0);
    height:40px !important;
    float:none !important;
    display:inline-block;
    *display:inline;
    *zoom:1;
}
.chart tr td>span, .chart .pc>span
{
    margin-top: 15% !important; margin-left: 2%; float: none !important;
    display:inline-block;
    vertical-align:top;
    *display:inline;
    *zoom:1;
}
.chart .pc .negative
{
    background-color:rgb(255,0,0)!important;
}
.chart .pc .positive
{
    background-color:rgb(0,255,0)!important;
}
.chart_wrapper
{
    margin:20px;
}
.collapser
{
    position:absolute; 
    top:0; 
    min-width:20px; 
    padding:0px 4px;
    font-size:1.5em;
    font-weight:bold;
    border-left:1px solid rgb(0,0,0); 
    border-right:1px solid rgb(0,0,0); 
    height:40px;
    cursor:pointer;
}
.btn-default
{
    background: none; border: 0px currentColor; text-decoration: none;
}
.dropdown-menu li a
{
    text-decoration: none;
}
.dropdown-menu
{
    border-radius: 0;
}
.removeme
{
    border: 0px currentColor; left: 0px; top: 0px; width: 100%; height: 100%; text-align: center; bottom: 0px; vertical-align: text-bottom; display: block; position: absolute; opacity: 0.5; background-color: rgb(255, 255, 255); border-image: none;
}
.removeme img
{
    margin: -2% auto auto; top: 50%; position: relative;
}
a.list-group-item {
    color: #555;
    cursor: pointer;
    text-decoration: none;
    font-size: 1.5em;
}
.panel-body
{
    position:relative;
}
.form-wrapper
{
    margin: 15% auto 0px; padding: 10px; width: 425px; clear: both; position: relative;
}