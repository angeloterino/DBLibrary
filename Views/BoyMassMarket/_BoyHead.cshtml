@model StrawmanApp.Models.BoyMassMarketModels

@{  string nname, tname = ViewData["tname"].ToString();
    StrawmanApp.Models.BoyMassMarketModels yitemaux = Model;
    nname = yitemaux.boy_name;

    <thead>
        @if (nname != tname && (yitemaux.brand < 9000 && yitemaux.market < 9000))
        {
            <tr><th colspan="3" style="font-size: .9em;">@nname&nbsp;</th></tr>
        }
    <tr style="background-color:rgb(0,102,204); margin-right:0;">
        <th style="font-size: .9em;">YTD14</th>
        <th style="font-size: .9em;">YTD15</th>
        <th style="font-size: .9em;">% CHGE</th>
    </tr>
    </thead>
}